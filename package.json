{
  "name": "turbo-gh-actions-test",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:ci": "turbo run build --cache-dir=.turbo",
    "dev": "turbo run dev --parallel",
    "check-types": "turbo run check-types",
    "lint": "turbo run lint",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "@scope/eslint-config-custom": "*",
    "prettier": "^2.6.2",
    "turbo": "^1.2.16"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.5.1"
}
